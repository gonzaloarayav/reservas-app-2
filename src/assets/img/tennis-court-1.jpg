<svg width="1200" height="600" viewBox="0 0 1200 600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4682B4;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="courtGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#228B22;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#006400;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="1200" height="300" fill="url(#skyGradient)"/>
  
  <!-- Court surface -->
  <rect x="0" y="300" width="1200" height="300" fill="url(#courtGradient)"/>
  
  <!-- Tennis court lines -->
  <!-- Outer court boundary -->
  <rect x="200" y="350" width="800" height="200" fill="none" stroke="white" stroke-width="4"/>
  
  <!-- Service boxes -->
  <line x1="600" y1="350" x2="600" y2="550" stroke="white" stroke-width="3"/>
  <line x1="200" y1="425" x2="1000" y2="425" stroke="white" stroke-width="3"/>
  <line x1="200" y1="475" x2="1000" y2="475" stroke="white" stroke-width="3"/>
  <line x1="400" y1="425" x2="400" y2="475" stroke="white" stroke-width="3"/>
  <line x1="800" y1="425" x2="800" y2="475" stroke="white" stroke-width="3"/>
  
  <!-- Net -->
  <rect x="595" y="350" width="10" height="200" fill="#333"/>
  <rect x="590" y="440" width="20" height="20" fill="#fff"/>
  
  <!-- Tennis balls -->
  <circle cx="350" cy="400" r="8" fill="#FFFF00" stroke="#FFA500" stroke-width="2"/>
  <circle cx="750" cy="500" r="8" fill="#FFFF00" stroke="#FFA500" stroke-width="2"/>
  
  <!-- Clouds -->
  <ellipse cx="300" cy="100" rx="60" ry="30" fill="white" opacity="0.8"/>
  <ellipse cx="320" cy="90" rx="40" ry="20" fill="white" opacity="0.8"/>
  <ellipse cx="900" cy="80" rx="80" ry="40" fill="white" opacity="0.8"/>
  <ellipse cx="920" cy="70" rx="50" ry="25" fill="white" opacity="0.8"/>
  
  <!-- Sun -->
  <circle cx="1000" cy="100" r="40" fill="#FFD700"/>
  <g stroke="#FFD700" stroke-width="3">
    <line x1="1000" y1="40" x2="1000" y2="20"/>
    <line x1="1000" y1="180" x2="1000" y2="160"/>
    <line x1="940" y1="100" x2="920" y2="100"/>
    <line x1="1080" y1="100" x2="1060" y2="100"/>
    <line x1="958" y1="58" x2="944" y2="44"/>
    <line x1="1042" y1="142" x2="1056" y2="156"/>
    <line x1="1042" y1="58" x2="1056" y2="44"/>
    <line x1="958" y1="142" x2="944" y2="156"/>
  </g>
</svg>