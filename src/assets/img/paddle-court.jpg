<svg width="1200" height="600" viewBox="0 0 1200 600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="eveningGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FF6B6B;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#4ECDC4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#45B7D1;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="paddleCourtGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#2E8B57;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#006400;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="1200" height="350" fill="url(#eveningGradient)"/>
  
  <!-- Court surface -->
  <rect x="0" y="350" width="1200" height="250" fill="url(#paddleCourtGradient)"/>
  
  <!-- Paddle court (smaller than tennis) -->
  <!-- Outer court boundary -->
  <rect x="300" y="380" width="600" height="140" fill="none" stroke="white" stroke-width="4"/>
  
  <!-- Center line -->
  <line x1="600" y1="380" x2="600" y2="520" stroke="white" stroke-width="3"/>
  
  <!-- Service lines -->
  <line x1="300" y1="450" x2="900" y2="450" stroke="white" stroke-width="2"/>
  <line x1="450" y1="380" x2="450" y2="520" stroke="white" stroke-width="2"/>
  <line x1="750" y1="380" x2="750" y2="520" stroke="white" stroke-width="2"/>
  
  <!-- Glass walls (paddle court characteristic) -->
  <rect x="280" y="360" width="20" height="180" fill="rgba(173,216,230,0.6)" stroke="#4682B4" stroke-width="2"/>
  <rect x="900" y="360" width="20" height="180" fill="rgba(173,216,230,0.6)" stroke="#4682B4" stroke-width="2"/>
  <rect x="300" y="360" width="600" height="20" fill="rgba(173,216,230,0.6)" stroke="#4682B4" stroke-width="2"/>
  <rect x="300" y="520" width="600" height="20" fill="rgba(173,216,230,0.6)" stroke="#4682B4" stroke-width="2"/>
  
  <!-- Net -->
  <rect x="595" y="380" width="10" height="140" fill="#333"/>
  <rect x="590" y="440" width="20" height="20" fill="#fff"/>
  
  <!-- Paddle rackets -->
  <ellipse cx="400" cy="420" rx="15" ry="25" fill="#8B4513" transform="rotate(30 400 420)"/>
  <rect x="395" y="440" width="10" height="30" fill="#654321" transform="rotate(30 400 420)"/>
  
  <ellipse cx="800" cy="480" rx="15" ry="25" fill="#8B4513" transform="rotate(-45 800 480)"/>
  <rect x="795" y="500" width="10" height="30" fill="#654321" transform="rotate(-45 800 480)"/>
  
  <!-- Paddle ball -->
  <circle cx="550" cy="460" r="6" fill="#FFFF00" stroke="#FFA500" stroke-width="1"/>
  
  <!-- Stadium lights -->
  <rect x="150" y="200" width="20" height="100" fill="#666"/>
  <circle cx="160" cy="190" r="25" fill="#FFFF99" opacity="0.8"/>
  
  <rect x="1030" y="200" width="20" height="100" fill="#666"/>
  <circle cx="1040" cy="190" r="25" fill="#FFFF99" opacity="0.8"/>
  
  <!-- Evening clouds -->
  <ellipse cx="200" cy="120" rx="50" ry="25" fill="rgba(255,255,255,0.7)"/>
  <ellipse cx="1000" cy="100" rx="70" ry="35" fill="rgba(255,255,255,0.7)"/>
</svg>